{
	"useTabs": true,
	"tabWidth": 2,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"printWidth": 100,
	"arrowParens": "always",
	"bracketSpacing": true,
	"endOfLine": "lf",
	"overrides": [
		{
			"files": ["*.ts", "*.tsx"],
			"options": {
				"parser": "typescript"
			}
		},
		{
			"files": ["*.json"],
			"options": {
				"tabWidth": 2
			}
		}
	]
}
